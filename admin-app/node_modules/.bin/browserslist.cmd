@echo off
call :find_dp0

if exist "%dp0%\node.exe" (
  set "_prog=%dp0%\node.exe"
) else (
  set "_prog=node"
  set "PATHEXT=%PATHEXT:;.JS;=;%"
)

endlocal & (title %COMSPEC% & "%_prog%" "%dp0%\..\browserslist\cli.js" %*) || goto :error

:find_dp0
set "dp0=%~dp0"
exit /b

:error
echo.
echo Error: The command is not recognized as an internal or external command.
echo.
pause
